<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="code.signUp">
   <children>

      <Button fx:id="btnSingUp" layoutX="10.0" layoutY="185.0" onMouseClicked="#buttonSignUpClick" mnemonicParsing="false" text="Sign Up"/>

       <Button fx:id="btnCancel" layoutX="75.0" layoutY="185.0" onMouseClicked="#buttonCancelClick" mnemonicParsing="false" text="Cancel" />

      <CheckBox fx:id="checkOrg" layoutX="10.0" layoutY="160.0" mnemonicParsing="false" text="Organization" />

      <TextField fx:id="textName" layoutX="10.0" layoutY="10.0" promptText="Name"/>

       <TextField fx:id="textPhone" layoutX="10.0" layoutY="40.0" promptText="Phone"/>

      <TextField fx:id="textEmail" layoutX="10.0" layoutY="70.0" promptText="e-mail"/>

      <TextField fx:id="textPass" layoutX="10.0" layoutY="100.0" promptText="Password"/>

      <TextField fx:id="textRePass" layoutX="10.0" layoutY="130.0" promptText="Retype password"/>

   </children>
</Pane>
